{
  "name": "csv_mail",
  "version": "1.0.0",
  "description": "Test script for connecting to the Roundcube Webmail server and extracting CSV-Files in order to further process and persist those",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf ./build && npx tsc",
    "start:dev": "npx nodemon",
    "start": "npm run build && node build/index.js",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint --fix src/**/*.ts",
    "prettier": "npx prettier --config .prettierrc 'src/**/*.ts'",
    "prettier:write": "npx prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier:watch": "npx onchange 'src/**/*.ts' -- npx prettier --write {{changed}}"
  },
  "author": "Katharina Borys/Giulia Winterhoff",
  "license": "ISC",
  "devDependencies": {
    "@types/base64-stream": "^1.0.0",
    "@types/convict": "^5.2.1",
    "@types/imap-simple": "^4.2.3",
    "@types/luxon": "^1.24.4",
    "@types/node": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "onchange": "^7.0.2",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "convict": "^6.0.0",
    "data-forge": "^1.8.11",
    "iconv-lite": "^0.6.2",
    "imap-simple": "^5.0.0",
    "luxon": "^1.25.0",
    "nodemon": "^2.0.4"
  }
}
